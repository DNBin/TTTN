@model QuanlyDiemrenluyen.Models.TaiKhoan

@{
    ViewBag.Title = "Thông tin";
    Layout = null;
}



<div class="modal-body">
    <h4>@ViewBag.Title</h4>
    <dl class="dl-horizontal">
        <dt>
            Mã tài khoản
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MaTaiKhoan)
        </dd>

        <dt>
            Tên tài khoản
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TenTaiKhoan)
        </dd>

        <dt>
            Ngày sinh
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NgaySinh)
        </dd>
        <dt>
            Ngày tạo
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NgayTao)
        </dd>

        <dt>
            Họ và tên
        </dt>

        <dd>
            @Html.DisplayFor(model => model.HoTen)
        </dd>

        <dt>
            Số điện thoại
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SoDienThoai)
        </dd>

        <dt>
            Email
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>

        <dt>
            Địa chỉ
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DiaChi)
        </dd>
        <dt>
            Vai trò
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MaQuyenNavigation.TenQuyen)
        </dd>

    </dl>
</div>
<div class="form-group">
    <div class="modal-footer">
        <button type="button" class="btn btn-light" data-bs-dismiss="modal">Đóng</button>
    </div>
</div>
